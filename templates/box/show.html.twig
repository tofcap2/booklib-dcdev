{% extends 'base.html.twig' %}

{% block title %}Box{% endblock %}

{% block body %}
    <h1>Box</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ box.id }}</td>
            </tr>
            <tr>
                <th>Latitude</th>
                <td>{{ box.latitude }}</td>
            </tr>
            <tr>
                <th>Longitude</th>
                <td>{{ box.longitude }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ box.address }}</td>
            </tr>
            <tr>
                <th>ZipCode</th>
                <td>{{ box.zipCode }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ box.city }}</td>
            </tr>
            <tr>
                <th>Comment</th>
                <td>{{ box.comment }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('box_index') }}">back to list</a>

    <a href="{{ path('box_edit', {'id': box.id}) }}">edit</a>

    {{ include('box/_delete_form.html.twig') }}
{% endblock %}
